{
    "name": "@universal-resume/ts-schema",
    "version": "0.2.0",
    "type": "module",
    "description": "An Effect based typescript implementation of the universal-resume standard, including structures, validatation and typing",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": ["dist"],
    "exports": {
        ".": "./dist/index.js"
    },
    "scripts": {
        "build": "tsc",
        "check": "biome check .",
        "format": "biome format . --write",
        "lint": "biome lint .",
        "test": "vitest",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/universal-resume/ts-schema.git"
    },
    "keywords": [
        "effect",
        "typescript",
        "resume",
        "cv",
        "schema",
        "universal-resume",
        "validation"
    ],
    "author": "Alexis Besson",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/universal-resume/ts-schema/issues"
    },
    "homepage": "https://github.com/universal-resume/ts-schema#readme",
    "dependencies": {
        "effect": "^3.17.2",
        "typescript": "^5.8.3"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.1.2",
        "vitest": "^3.2.4"
    },
    "publishConfig": {
        "access": "public"
    }
}