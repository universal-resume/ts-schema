{
	"compilerOptions": {
		"baseUrl": ".",
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "Node",
		"declaration": true,
		"outDir": "dist",
		"rootDir": "src",
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
        "paths": {
			"#value-object": [
				"./src/value-object.ts"
			]
        }
	},
	"include": ["src"]
}
